{
  "name": "group-ironmen",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "axios": "^1.9.0",
    "chokidar": "^4.0.3",
    "clean-css": "^5.3.3",
    "compression": "^1.8.0",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "express": "^5.1.0",
    "express-winston": "^4.2.0",
    "glob": "^11.0.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "terser": "^5.39.2",
    "winston": "^3.17.0"
  },
  "scripts": {
    "bundle": "npm run clean && node build.js --prod",
    "clean": "node scripts/clean.js",
    "start": "concurrently -P \"npm run watch\" \"npm run serve -- --backend {1}\" --",
    "start:local-api": "concurrently \"npm run watch\" \"npm run serve -- \"--backend 127.0.0.1:8080\"\"",
    "serve": "node scripts/server.js",
    "watch": "node build --watch",
    "lint": "eslint --ext .js src",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "npm run lint -- --fix",
    "generate-component": "node scripts/generate-component.js",
    "prepare": "cd .. && husky site/.husky",
    "precommit": "npm run format:check && npm run lint"
  },
  "author": "Christopher Brown",
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.25.4"
  }
